{
    "name": "atepam/laravel-alphavantage-client",
    "description": "AlphaVantage Client for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Atepam\\AlphavantageClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atepam\\AlphavantageClient\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Andor Tittel",
            "email": "andor_tittel@epam.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "php": "^8.1",
        "eventsauce/object-hydrator": "^1.4",
        "illuminate/console": "^11.16.0",
        "illuminate/support": "^11.16"
    },
    "require-dev": {
        "laravel/pint": "^1.16",
        "orchestra/testbench": "^9.2",
        "pestphp/pest": "^2.34",
        "phpstan/phpstan": "^1.11"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atepam\\AlphavantageClient\\Providers\\PackageServiceProvider"
            ]
        }
    }
}
